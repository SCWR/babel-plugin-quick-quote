  "description": "easy to load and import echarts module on demand",
  "author": "SCWR(dl_run@qq.com)",
  "main": "lib/index.js",
  "scripts": {
    "start": "yarn build && yarn test -u",
    "build": "babel src --out-dir lib --presets=@babel/preset-env",
